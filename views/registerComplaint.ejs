<body>
  <nav class="blue">
    <div class="nav-wrapper">
      <a class="brand-logo center white-text"><img src="images/logo.png" width="100px"> A Plus Help Desk</a>
      <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li>
          <a href="/history">History</a>
        </li>
        <li><a href="/logout">Logout</a></li>
      </ul>
      <ul class="side-nav" id="mobile-demo">
        <li><a href="/history">History</a></li>
        <li><a href="/logout">Logout</a></li>
      </ul>
    </div>

  </nav>
  <br/><br/>
  <center><div class="chip" class="center-align">Welcome, <%= name %></div></center>
<br/><br/>
  <p class="center-align black-text "><em>Select your complaint type:</em></p>
  <% if (JSON.stringify(data)) { %>
  <p style="color:green" class="center-align"><%= data %></p>
  <% } %>
  <form action="/submit" method="post" id="form-remedy">
   <% if (JSON.stringify(email)) { %>
   <input type="text" value="<%= email %>" name="user" hidden required>
   <% } %>

   <div class="row">
    <div class="col s0 m0 l3 xl3">&nbsp;   </div> 
    <div class="col s12 m12 l6 xl6 center-align">
      <select class="browser-default" name="category" required>
        <option value="" disabled selected>Choose category</option>
        <option value="Internet related">Internet related</option>
        <option value="Hardware support">Hardware support</option>
        <option value="Software support">Software support</option>
        <option value="Stationary">Stationary</option>
        <option value="Equipment">Equipment</option>
        <option value="Server related">Server related</option>
        <option value="Others">Others</option>
      </select>
    </div>
  </div>
  <p class="center-align black-text "><em>Enter Detailed Description</em></p>
  <div class="row">
    <div class="col s0 m0 l3 xl3">&nbsp;   </div> 

    <div class="input-field col s12 m12 l6 xl6 center-align">
      <textarea id="textarea1" class="materialize-textarea" name="description" required></textarea>
      
    </div>
  </div>
  <div class="row">
    <div class="col s0 m0 l3 xl3">&nbsp;   </div> 

    <div class="col s12 m12 l6 xl6 center-align">
      <button class="waves-effect waves-light btn" type="submit">Register Complaint</button>
    </div>
  </div>
</form>

<br/><br/><br/><br/>

<footer class="page-footer blue">
  <div class="footer-copyright">
    <div class="container">
