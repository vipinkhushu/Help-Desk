  <p class="center-align black-text ">Register new user</p>
  <% if (data) { %>
  <p style="color:green" class="center-align"><%= data %></p>
<% } %>
  <form action="register" method="post" id="form-register">
  <div class="row">
    <div class="col s0 m0 l3 xl3">&nbsp;   </div> 
    <div class="col s12 m12 l6 xl6 center-align">
      <select class="browser-default" name="category">
      <option value="" disabled selected>Choose department</option>
        <option value="Accounts">Accounts</option>
        <option value="Sales">Sales</option>
        <option value="Operation">Operation</option>
        <option value="Management">Management</option>
        <option value="HR">Human Resource</option>
        <option value="Directors">Directors</option>
        <option value="Others">Others</option>
      </select>
    </div>
  </div>
  <div class="row">
    <div class="col s0 m0 l3 xl3">&nbsp;   </div> 

    <div class="input-field col s12 m12 l6 xl6 center-align">
          <input placeholder="Name" id="name" name="name" type="text" class="validate">
          <label for="name">Name</label>
    </div>
  </div>
  <div class="row">
    <div class="col s0 m0 l3 xl3">&nbsp;   </div> 

    <div class="input-field col s12 m12 l6 xl6 center-align">
          <input placeholder="Email" id="email" name="email" type="email" class="validate">
          <label for="email">Email</label>
    </div>
  </div>

  <div class="row">
    <div class="col s0 m0 l3 xl3">&nbsp;   </div> 

    <div class="input-field col s12 m12 l6 xl6 center-align">
          <input placeholder="Password" id="password" name="password" type="password" class="validate">
          <label for="password">Password</label>
    </div>
  </div>

  <div class="row">
      <div class="col s0 m0 l3 xl3">&nbsp;   </div> 

    <div class="col s12 m12 l6 xl6 center-align">
      <a class="waves-effect waves-light btn" onclick="document.getElementById('form-register').submit();">Register New User</a>
    </div>
  </div>
  </form>
<br/><br/><br/><br/>
