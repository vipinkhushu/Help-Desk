<script type="text/javascript">
  $(document).ready(function(){
    // the "href" attribute of the modal trigger must specify the modal ID that wants to be triggered
    $('.modal').modal();

  });
  function openModalWithDetails(desc){
    $('#modalbody').html(desc);
  }
</script>
