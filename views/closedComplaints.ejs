 </nav>
 <p class="center-align black-text ">All Closed Complaints</p>
 <div id="table-data">
  <table class="bordered highlight responsive-table">
    <thead>
      <tr>
        <th>ID</th>
        <th>Category</th>
        <th>User</th>
        <th>Time stamp</th>
        <th>Status</th>
        <th>Description</th>
        
      </tr>
    </thead>

    <tbody>
      <% for(x in data){ %>
      <tr>
        <td><%= data[x]._id %></td>
        <td><%= data[x].category %></td>
        <td><%= data[x].user %></td>
        <td><%= data[x].time %></td>
        <td><%= data[x].status %></td>
        <td><a class="waves-effect waves-light btn modal-trigger" href="#modal1" onclick="document.getElementById('modalbody').innerHTML='<%= data[x].description %>'"">Details</a></td>
        
      </tr>
      <% } %>
    </tbody>
  </table>
  <div id="modal1" class="modal">
    <div class="modal-content">
      <h4 id="modalhead">MESSAGE:</h4>
      <p id="modalbody">A bunch of text</p>
    </div>
    <div class="modal-footer">
      <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat">Close</a>
    </div>
  </div>
</div>



